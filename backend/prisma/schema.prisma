generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type UfsDescriptionType {
  gender          String
  age             Int
  course          String
  campus          String
  healthGraduated String
  knowGuia        String
  opinionAbout    String?
}

type SchoolDescriptionType {
  gender       String
  age          Int
  state        String
  knowGuia     String
  opinionAbout String?
}

type QuizType {
  question1  Int
  question2  Int
  question3  Int
  question4  Int
  question5  Int
  question6  Int
  question7  Int
  question8  Int
  question9  Int
  question10 Int
  question11 Int
  question12 Int
  question13 Int
  question14 Int
  question15 Int
  question16 Int
  question17 Int
  question18 Int
  question19 Int
  question20 Int
  question21 Int
  question22 Int
  question23 Int
  question24 Int
  corrects   Int
}

model Course {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  name String
}

model UfsUser {
  id            String             @id @default(auto()) @map("_id") @db.ObjectId
  termsAccepted Boolean
  description   UfsDescriptionType
  quiz          QuizType
  createdAt     DateTime           @default(now())
}

model SchoolUser {
  id            String                @id @default(auto()) @map("_id") @db.ObjectId
  termsAccepted Boolean
  description   SchoolDescriptionType
  quiz          QuizType
  createdAt     DateTime              @default(now())
}
